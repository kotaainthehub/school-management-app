    <query language="SQL"><![CDATA[
        SELECT 
            r.report_id AS id,
            s.name,
            s.class,
            s.roll AS rollnumber,
            r.grade,
            r.phy,
            r.chem,
            r.math,
            ROUND(((r.phy + r.chem + r.math) * 100.0) / 300, 2) AS growth_percentage
        FROM reportcard r
        JOIN stureg s ON r.student_id = s.student_id
    ]]></query>

